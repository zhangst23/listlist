<ul class="lists box">
	<% lists.each do |list| %>
		<li>
			<h2 class="list_title">
				<%= link_to list.title, list %>
			</h2>
			<p>
	          <%= link_to list.node.name, list.node, class: "node" %>
	          <span> . </span>
	          <%= link_to "user.name", class: "user_link"%>
	        </p>
		</li>
	<% end %>
</ul>