<div class="row">
  <div class="medium-8 columns">
    <% @lists.each do |list| %>
      <div class="list">
        <h3><%= link_to list.title, list %></h3>
        <p><%= truncate(list.content, length:99) %></p>
      </div>
    <% end %> 
    <br>
    <%= paginate @lists %>
  </div>
  

  <div class="medium-4 columns">
    <%= link_to '创建清单', new_list_path, class: "disabled button" %>
    <br>
  	<%= render "/shared/side_node" %>
  </div>
  
</div>








